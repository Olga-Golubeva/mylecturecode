package lv.olga;

public class LearnMethodsWithType {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		getArea();
		System.out.println(getArea());
	
	}
	// method return type double
	// a*b =result
	
	public static int getArea(){
		
		double num1 = 20.3;
		double num2 = 10;
		double result = num1*num2;
		
		return result;
		
		
	}
	

}
