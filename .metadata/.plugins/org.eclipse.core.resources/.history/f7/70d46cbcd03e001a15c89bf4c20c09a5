package lv.olga;

public class LearnMethodsWithType {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		double area = getArea();
		
	
	}
	// method return type double
	// a*b =result
	
	public static double getArea(){
		
		double num1 = 20.3;
		double num2 = 10;
		double result = num1*num2;
		System.out.println("Area of rectangle is " +result);
		return result;
		
		
	}
	

}
