
public class Calculator {

	
	public static void main(String[] args) {
		
		char operatorMath = '+';
		
		double num1 = 20.0;
		double num2 = 24.5;
		
		if(operatorMath =='+'){
			System.out.println(num1 + num2);
		
		}else if(){	
			}
	}else if(operatorMath =='-')
	{System.out.println(num1 - num2);}
	}else if(){	}
	}else if(){	}
	
		}else{
			System.out.println("FALSE");
		}
	}
	

}
