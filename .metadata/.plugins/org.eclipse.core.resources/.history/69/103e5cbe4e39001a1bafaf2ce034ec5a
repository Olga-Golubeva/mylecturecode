
public class PrimType {

	
	public static void main(String[] args) {
		// Numeric data primitive type
		int integer = 2;
		long longInter = 276387564478L;
		short shortInteger = 2020;
		byte byteInteger = 120;
		//Floating-Point type real number
		double doubleFloutNumber = 0.5d;
		float floatNumber = 0.12412412f;
		// Characters
		char charSymbol = 'a';
		char charSymbolAsNumber = 79;
		char charSymbolAsNumber1 = 76;
		char charSymbolAsNumber2 = 71;
		char charSymbolAsNumber3 = 65;
		
		System.out.println(charSymbolAsNumber+charSymbolAsNumber1+charSymbolAsNumber2+charSymbolAsNumber3);
		
	}

}
