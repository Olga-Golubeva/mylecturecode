package lv.olga;

import java.util.ArrayList;

public class TestTaskArray {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	
		ArrayList<String> cars = new ArrayList<>();
		cars.add("BMW");
		cars.add("VW");
		cars.add("Volvo");
		cars.add("Opel");
		cars.add("Ferrari");
		cars.add("Ford");
		
		for(String car: cars);
		
		System.out.println(car);

	}

}
