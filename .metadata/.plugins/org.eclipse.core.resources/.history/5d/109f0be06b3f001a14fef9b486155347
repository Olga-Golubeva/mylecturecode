package lv.olga.learnstring;

public class TestStringClass {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		String str = "Index";
		String strClass = new String("Index param");
		
		
		System.out.println(str);
		
		System.out.println("String like object " + strClass);

	}

}
