
public class ArraysAndLoop {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		//Declare variable
		int[] marks = {76, 55, 34, 69, 4, 100, 10};
		//Make toString all elements of array
		//Print in console
		
		for(int i = 0; i <marks.length; i++){
			
			System.out.println(marks[i]);
		}
	
	}

}
