
public class OperatorsMath {


	public static void main(String[] args) {
		double num1 = 0.4;
		double num2 = 0.20;
		double result;
		double result1;
		double result2;
		char operators = '+';
		char sub = '-';
		
		
		result = num1 + num2;
		result1 = num1 - num2;
		result = num1 * num2;
		
		 
		System.out.println(num1+""+operators+""+ num2 +"="+result);
		System.out.println(num1+""+sub+""+ num2 +"="+result1);
		System.out.println(); 
		
		
		int a = 11;
		int b = 3;
		int x = a % b;
		
		System.out.println("x = "+x);
		
		int i = 6;
		i++;
		System.out.println(i);
		
	}

}
