		System.out.println("Input the name");
		
		strArr[0] = sc.nextLine();
		
		System.out.println("Input the name");
		
		strArr[1] = sc.nextLine();
		
System.out.println("Input the name");
		
		strArr[2] = sc.nextLine();
		
		
		
	}
	

}
